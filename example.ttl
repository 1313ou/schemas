  schema:url <http://globalwordnet.github.io/schemas/> ;
  dc:publisher <Global Wordnet Association> .

<#w4> a lemon:LexicalEntry ;
  lemon:canonicalForm [ lemon:writtenRep "farfar"@en ] ;
  wn:partOfSpeech wn:noun ;
  lemon:sense <#example-2-n-1> .

<#example-2-n-1> a lemon:LexicalSense ;
  lemon:reference <#example-1-n> ;
  wn:senseExample [
   dc:source "Europarl Corpus"@en ;
   lemon:value "Jag vill berätta för er att min farfar var svensk beredskapssoldat vid norska gränsen under andra världskriget, ett krig som Sverige stod utanför"@sv
  ] .
