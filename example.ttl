<#example-en> a lemon:Lexicon ;
  rdfs:label "Example wordnet"@en ;
  dc:language "en"@en ;
  schema:email "john@mccr.ae" ;
  dc:rights <https://creativecommons.org/publicdomain/zero/1.0/> ;
  owl:versionInfo "1.0" ;
  schema:citation "CILI: the Collaborative Interlingual Index. Francis Bond, Piek Vossen, John P. McCrae and Christiane Fellbaum, Proceedings of the Global WordNet Conference 2016, (2016)."@en ;
  schema:url <http://globalwordnet.github.io/schemas/> ;
  dc:publisher "Global Wordnet Association"@en ;
  lemon:entry <#w1>, <#w2>, <#w3> .
        
<#w1> a lemon:LexicalEntry ;
  lemon:canonicalForm [ lemon:writtenRep "grandfather"@en ] ;
  wn:partOfSpeech wn:noun ;
  lemon:sense <#example-10161911-n-1> .

<#example-10161911-n-1> a lemon:LexicalSense ;
  lemon:reference <example-10161911-n> .

<#w2> a lemon:LexicalEntry ;
  lemon:canonicalForm [ lemon:writtenRep "paternal grandfather"@en ] ;
  wn:partOfSpeech wn:noun ;
  lemon:sense <#example-1-n-1> .

<#example-1-n-1> a lemon:LexicalSense ;
  lemon:reference <#example-1-n> ;
  wn:derivation <example-10161911-n-1> .

<#w3> a lemon:LexicalEntry ;
  lemon:canonicalForm [ lemon:writtenRep "pay"@en ] ;
// Be careful with spelling here:
  lemon:synBehavior 
    [ rdfs:label "Sam cannot %s Sue" ] ,
    [ rdfs:label "Sam and Sue %s" ] ,
    [ rdfs:label "The banks %s the check" ] .

<#example-10161911-n> a skos:Concept ;
  owl:sameAs ili:i90287 ;
  lemon:definition [
    lemon:value "the father of your father or mother"@en 
  ] ;
  wn:hypernym <#example-10162692-n> .

<#example-1-n> a skos:Concept ;
// This is for proposing a new synset! Do not publish with this annotation.
  owl:sameAs ili:in ;
  lemon:definition [
    lemon:value "A father's father; a paternal grandfather"@en ;
  ] ,
  wn:iliDefinition [
    dc:source <https://en.wiktionary.org/wiki/farfar> ;
    lemon:value "A father's father; a paternal grandfather"@ en
  ] .
        
<#example-10162692-n> owl:sameAs ili:i90292" .
    
